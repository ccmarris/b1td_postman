		{
			"name": "Reporting",
			"item": [
				{
					"name": "Get RPZ Hits for last 15 minutes",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var moment = require(\"moment\")",
									"var sif1 = moment().unix(); ",
									"var sif2 = moment().subtract(7, 'days').unix()",
									"pm.environment.set(\"time1\", sif1);",
									"pm.environment.set(\"time0\", sif2);",
									"console.log(sif1)",
									"console.log(sif2)"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "",
								"value": "",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/dnsdata/v2/dns_event?t0={{time0}}&_format=json&t1={{time1}}&source=rpz",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"dnsdata",
								"v2",
								"dns_event"
							],
							"query": [
								{
									"key": "t0",
									"value": "{{time0}}"
								},
								{
									"key": "_format",
									"value": "json"
								},
								{
									"key": "t1",
									"value": "{{time1}}"
								},
								{
									"key": "source",
									"value": "rpz"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "Get Category Hits for the Last Hour",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var moment = require(\"moment\")",
									"var sif1 = moment().unix(); ",
									"var sif2 = moment().subtract(1, 'hours').unix()",
									"pm.environment.set(\"time1\", sif1);",
									"pm.environment.set(\"time0\", sif2);",
									"console.log(sif1)",
									"console.log(sif2)"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
